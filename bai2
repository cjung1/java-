package tech7;
import java.util.Scanner;

public class b2p2_2 {
public static void main(String[]args) {
	Scanner e = new Scanner(System.in);
	int a = e.nextInt();
	// cau 1
	System.out.println("cau 1 ");
	if(a%2==0) {
		System.out.println("YES");
	}
	else {
		System.out.println("NO");
	}
	// cau 2
	System.out.println("cau 2 ");
	if(a%3==0&&a%5==0) {
		System.out.println("YES");
	}
	else {
		System.out.println("NO");
	}
	// cau 3
	System.out.println("cau 3 ");
	if(a%3==0&&a%7!=0) {
		System.out.println("YES");
	}
	else {
		System.out.println("NO");
	}
	// cau 4 
	System.out.println("cau 4 ");
	if(a%3==0||a%7==0) {
		System.out.println("YES");
	}
	else {
		System.out.println("NO");
	}
	// cau 5
	System.out.println("cau 5 ");
	if(a>30&&a<50) {
		System.out.println("YES");
	}
	else {
		System.out.println("NO");
	}
	// cau 6
	System.out.println("cau 6 ");
	if(a>=30) {
		if(a%2==0||a%3==0) {
			System.out.println("YES");
		}
		else if(a%5==0) {
			System.out.println("YES");
		}
		else {
			System.out.println("NO");
		}
	}
	else {
		System.out.println("NO");
	}
	// cau 7 
	System.out.println("cau 7 ");
	/*int g = 1;
	int c = a%100;
	double d = Math.sqrt(c);
	int f = (int)d;
	// System.out.println("f la "+ f);
	for(int i = 2;i<=f;i++) {
		if(c%i==0) {
			System.out.println("NO");
			g = 0;
			break;
		}
	}
	if(g == 1) {
		System.out.println("YES");
	}*/
	int c = a%10;
	if(a>=10&&a<=99) {
		if(c==2||c==3||c==5||c==7) {
			System.out.println("YES");
		}
		else {
			System.out.println("NO");
		}
	}
	else {
		System.out.println("NO");
	}
	// cau 8
	System.out.println("cau 8 ");
	if(a<=100&&a%23 ==0) {
		System.out.println("YES");
	}
	else {
		System.out.println("NO");
	}
	// cau 9
	System.out.println("cau 9 ");
	if(a>20||a<10) {
		System.out.println("YES");
	}
	else {
		System.out.println("NO");
	}
	// cau 10
	System.out.println("cau 10 ");
	int b = a%10;
	if(b%3==0) {
		System.out.println("YES");
	}
	else {
		System.out.println("NO");
	}
}
}
