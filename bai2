package tech7;
import java.util.Scanner;
public class b2p2 {
public static void main(String []args) {
	Scanner a = new Scanner(System.in);
	int b = a.nextInt();
	if(b%400==0||(b%4==0&&b%100!=0)) {
		System.out.println("YES");
	}
	else {
		System.out.println("NO");
	}
}

}
